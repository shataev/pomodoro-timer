{
  "name": "pomodoro-timer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "babel:node": "./node_modules/.bin/babel-node --presets=@babel/env",
    "dev": "nodemon --exec npm run babel:node -- server/index",
    "serve:client": "vue-cli-service serve",
    "serve:server": "nodemon server",
    "serve": "concurrently \"npm run serve:*\" -k",
    "build": "vue-cli-service build",
    "test:unit:server": "mocha  --require @babel/register tests/unit/server"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.22",
    "validator": "^13.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-service": "^4.4.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.2.0",
    "expect": "^26.1.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "tailwindcss": "^1.4.6",
    "vue-template-compiler": "^2.6.11"
  }
}
